n            });\n    }\n\n    useEffect(() => {\n            if (user.photoURL) {\n                setImageUrl(user.photoURL);\n            } else {\n                setImageUrl(getDefaultImage());\n            }\n        }\n        , []);\n\n    useEffect(() => {\n            if (user.photoURL) {\n                setImageUrl(user.photoURL);\n            }\n        }\n        , [user.photoURL]);\n\n    return (\n        <img src={imageUrl} alt=\"Profile-pic could not load\" className={props.className}/>\n        // <img src={user.photoURL ? user.photoURL: getDefaultImage()} alt=\"ahhh\"/>\n    )\n}\n\nexport default ProfilePic","/Users/mandyhe/cs0320/term-project-awhite35-mcamach6-mhe26-pmontei1/frontend/src/components/NewSurvey.js",["129","130","131"],"import React, { useState, useEffect } from \"react\";\nimport * as RiIcons from \"react-icons/ri\";\nimport Survey from \"../Alana_stuff/Survey\";\n\n\nfunction NewSurvey(props) {\n    return (\n        <div className=\"addSurveyIcon\" >\n            <RiIcons.RiAddLine className=\"icon\" onClick={()=>props.setSurveyVisibility(true)}/>\n            <span className=\"labelIcon\"><h3> Click to create new survey</h3></span>\n        </div>\n\n    )\n}\n\nexport default NewSurvey","/Users/mandyhe/cs0320/term-project-awhite35-mcamach6-mhe26-pmontei1/frontend/src/pages/TakeSurvey.js",[],{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","severity":1,"message":"137","line":11,"column":8,"nodeType":"138","messageId":"139","endLine":11,"endColumn":14},{"ruleId":"136","severity":1,"message":"140","line":13,"column":8,"nodeType":"138","messageId":"139","endLine":13,"endColumn":12},{"ruleId":"141","severity":1,"message":"142","line":51,"column":17,"nodeType":"143","messageId":"144","endLine":60,"endColumn":18},{"ruleId":"141","severity":1,"message":"142","line":77,"column":17,"nodeType":"143","messageId":"144","endLine":87,"endColumn":18},{"ruleId":"145","severity":1,"message":"146","line":11